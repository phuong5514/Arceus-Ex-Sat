    "dotenv": "^16.4.7",
    "ejs": "^3.1.10",
    "express": "^4.21.2",
    "express-fileupload": "^1.5.1",
    "express-validator": "^7.2.1",
    "mongoose": "^8.12.1",
    "mongoose-aggregate-paginate-v2": "^1.1.4",
    "mongoose-paginate-v2": "^1.9.0",
    "nodemon": "^3.1.9",
    "pocketbase": "^0.25.2",
    "puppeteer": "^24.6.1",
    "zod": "^3.24.2"
